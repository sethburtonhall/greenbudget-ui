<script src="/js/jquery.min.js"></script>
<script src="/js/popper.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/scripts.js"></script>
<script src="/js/custom-form-validation.js"></script>
<script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>

<script type="text/javascript">
    $('.calendar').flatpickr({
        altInput: true,
        dateFormat: 'F, d Y',
        nextArrow: '<img src="/img/icons/next.svg" alt="next" onload="SVGInject(this)">',
        prevArrow: '<img src="/img/icons/previous.svg" alt="previous" onload="SVGInject(this)">'
    });

    $('#shootDateRange').flatpickr({
        altInput: true,
        dateFormat: 'F, d Y',
        nextArrow: '<img src="/img/icons/next.svg" alt="next" onload="SVGInject(this)">',
        prevArrow: '<img src="/img/icons/previous.svg" alt="previous" onload="SVGInject(this)">',
        mode: "range"
    });
</script>
